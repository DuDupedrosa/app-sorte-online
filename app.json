{
  "expo": {
    "name": "loto-online",
    "slug": "loto-online",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/images/clover-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/clover-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eduardopedrosa.lotoonline"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "454c19e3-76f0-4fd8-9d11-6e3ed595e296"
      }
    }
  }
}
